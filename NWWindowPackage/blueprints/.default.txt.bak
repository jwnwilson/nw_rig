window = NWWindow.NWWindow({},name= self.name, windowWidth = 400, windowHeight = 400, title = "NWWindow", NWRig = self)
window.layout({'label':'root'})
createTab = window.layout({'label':'tab1','type':'tabLayout'})
frame1 = window.layout({'parent':createTab,'label':'create','type':'frameLayout'})
window.text({'label':'create menu'})
window.button({'label':'New'})
window.button({'label':'Save'})
window.button({'label':'Load'})
startTab = window.layout({'parent':createTab,'label':'starter','type':'frameLayout'})
window.text({'label':'Load starter'})
scroll1 = window.layout({'type':'scrollLayout','parent':startTab, 'label':'starters'})
window.loadStarters(NWWindowPackage.__path__)
window.text({'label':'Module name','parent':startTab})
window.textField({'label':'starterName','parent':startTab})
buildTab = window.layout({'parent':createTab,'label':'build','type':'frameLayout'})
window.text({'label':'build menu'})
buildScroll1 = window.layout({'type':'scrollLayout','parent':buildTab, 'label':'buildscroll'})
buildIconScrollList = window.loadBuilds({'parent':buildScroll1})
buildAttributes = window.loadBuildAttributes({'parent':buildScroll1})
window.button({'parent':buildTab, 'label':'refresh',"command":"NWRig.UI.loadBuilds({})"})
window.button({'parent':buildTab, 'label':'build',"command":"NWRig.UI.buildRecursive({})"})
